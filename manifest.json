{
	"update_url": "https://clients2.google.com/service/update2/crx",
	
			"manifest_version": 3,
			"version": "1.1.3",
			"name": "__MSG_appName__",
			"default_locale": "en",
			"description": "__MSG_appDesc__",
			"icons": { 
				"128": "/img/128.png"
			},
			"background": {
				"service_worker": "/js/bg.js"
				
			},
			"permissions": [
   				 "search"
  			],
			"action": {
					"browser_style": false,
					"default_icon": {
							"128": "/img/128.png"
					},
					"default_title": "New Tab"
	
			},
			"chrome_url_overrides": {
				"newtab": "index.html"
			}
	}

